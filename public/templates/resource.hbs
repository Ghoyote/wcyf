<section class="no-padding bg-dark" id="portfolio">
	<div class="container-fluid">
		<div class="row">
			<div class="col-lg-12 text-center">
				<h2 class="section-heading" style="margin-top:85px">More From The Blog</h2>
				<hr class="primary">
			</div>
		</div>
		<div class="row no-gutter popup-gallery" style="margin-bottom:50px">
			<div class="col-lg-4 col-sm-6 port-item" ng-repeat="res in resources.resource | limitTo: 6">
				<a ng-href="res.image.full">
					<div class="portfolio-box col-sm-6">
						<img ng-src="{[{res.image.thumb}]}" class="img-responsive" alt="">
					</div>
					<blockquote class="col-sm-6" style="color:black" ng-bind="res.description">One must shape the believer in order to hurt the master of atomic anger.</blockquote>
				</a>
			</div>
		</div>
	</div>
</section>